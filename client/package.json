{
  "name": "closterwebapplication",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@craco/craco": "^6.1.2",
    "axios": "^0.24.0",
    "history": "^5.1.0",
    "peerjs": "^1.3.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-query": "^3.32.1",
    "react-router-dom": "^6.0.2",
    "react-scripts": "4.0.3",
    "socket.io-client": "^4.3.2"
  },
  "scripts": {
    "start": "PORT=10000 craco start",
